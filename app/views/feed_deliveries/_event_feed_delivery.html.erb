<% delivery = event_feed_delivery %>
<% event = delivery.feed_item.originator %>
<div class='feed-list' id='feed_delivery_<%= delivery.id %>'>
  <div class="feed-icon icon-friend"></div>
  <p><%= profile_link event.poster %>组织了活动<%= event_link event %></p>
	<p> <%= ftime delivery.created_at %> </p>
  <div><%= yield %></div> 
</div>
